{
  "schemaVersion": 1,
  "id": "squake",
  "version": "${version}",
  "name": "squake",
  "description": "A mod that adds Quake movement to minecraft!",
  "authors": [
    "thederptastic"
  ],
  "contact": {
    "repo": "https://github.com/thederptastic/squakeupdated"
  },
  "license": "Unlicense",
  "icon": "assets/squake/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "derp.squake.client.SquakeFabricClient"
    ],
    "modmenu" : [
      "derp.squake.ModMenuIntegration"
    ],
    "main": [
      "derp.squake.SquakeFabric"
    ]
  },
  "mixins": [
    "squake.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "1.21.*",
    "cloth-config": "*"
  }
}
